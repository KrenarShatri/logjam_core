<% if !@dataset.apdex(:backend).nan? %>
<div class="item col-5 icon-action<% if "%.2f" % @dataset.apdex >= "0.94" %> apdex-ok<% end %><% if "%.2f" % @dataset.apdex < "0.94" %> apdex-fail<% end %>">
  <%= clean_link_to(:action => "apdex_overview", :resource => 'total_time') do %>
    <h2>Apdex Backend: <span class="number"><%= "%.2f" % @dataset.apdex(:backend) %></span></h2>
  <% end %>
  <table class="collapse-table full_stats">
    <tr title="<%= integer_number @dataset.happy_count(:backend) %> requests">
      <td>happy</td>
      <td class="number">
        <%= "%.2f" % (100.0 * @dataset.happy(:backend)) %>%
      </td>
    </tr>
    <tr title="<%= integer_number @dataset.satisfied_count(:backend) %> requests">
      <td>satisfied</td>
      <td class="number">
        <%= "%.2f" % (100.0 * @dataset.satisfied(:backend)) %>%
      </td>
    </tr>
    <tr title="<%= integer_number @dataset.tolerating_count(:backend) %> requests">
      <td>tolerating</td>
      <td class="number">
        <%= "%.2f" % (100.0 * @dataset.tolerating(:backend)) %>%
      </td>
    </tr>
    <tr title="<%= integer_number @dataset.frustrated_count(:backend) %> requests">
      <td>frustrated</td>
      <td class="number">
        <%= "%.2f" % (100.0 * @dataset.frustrated(:backend)) %>%
      </td>
    </tr>
  </table>
</div>
<% end %>


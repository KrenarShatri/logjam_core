<%
   data_with_colors = data.map{|d| d||0}.zip(colors)
   js_params = {
     w: (size rescue 100),
     h: (size rescue 100),
     legend: legend,
     data: data_with_colors.map(&:first),
     color: data_with_colors.map(&:second),
     container: "#" + container,
     onclick: onclick
   }
-%>
<script type="text/javascript">
logjam_simple_pie(<%=raw Oj.dump(js_params) %>);
</script>

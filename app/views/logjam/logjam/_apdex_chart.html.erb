<%
   js_params = {
     parent: "#apdex-chart-#{i}",
     max_x: apdex_score.keys.max || 0,
     max_y: apdex_score.values.max || 0,
     min_y: (apdex_score.values + [0.9]).min || 0,
     height: height,
   }
   js_params[:data] = (0..js_params[:max_x]).each_with_object({}){|i,h|h[i]=-1}.merge!(apdex_score).map{|k,v| [k,v]}
%>
<script type="text/javascript">
logjam_apdex_chart(<%= raw Oj.dump(js_params) %>);
</script>
